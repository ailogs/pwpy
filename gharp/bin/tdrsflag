#!/bin/tcsh

if ($#argv < 1) then
	echo "Must give a tdrs-dataset"
	exit(0)
endif

set cal=$1

# flag all cal data but good channels
#uvflag vis=$cal flagval=unflag
uvflag vis=$cal flagval=flag line=chan,1,513
uvflag vis=$cal flagval=flag line=chan,300,1
uvflag vis=$cal flagval=flag line=chan,400,625
uvflag vis=$cal flagval=flag select="ant(26,29)"

