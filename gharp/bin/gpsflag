#!/bin/tcsh

if ($#argv < 1) then
	echo "Must give a gps-dataset"
	exit(0)
endif

set cal=$1

# flag all cal data but good channels
#uvflag vis=$cal flagval=unflag
#uvflag vis=$cal flagval=flag line=chan,1,513
uvflag vis=$cal flagval=flag line=chan,150,1
uvflag vis=$cal flagval=flag line=chan,859,166

