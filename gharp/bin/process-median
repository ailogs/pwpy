#!/bin/bash

moon_files=`ls -d freqmosfx-moon-* | xargs`
cal_files=`ls -d freqmosfx-moon-* | xargs`

echo "$cal_files"
echo "$moon_files"


for cal in $cal_files 
do
  echo "java jmir.miriad.ACMedian vis=$cal out=ACCal.txt"
  java jmir.miriad.ACMedian vis=$cal out=ACCal.txt
done

for moon in $moon_files 
do
  echo "java jmir.miriad.ACMedian vis=$moon out=ACMoon.txt"
  java jmir.miriad.ACMedian vis=$moon out=ACMoon.txt
done

